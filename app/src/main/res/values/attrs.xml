<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="com.theglendales.alarm.view.VolumePreference">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="cancelButton" format="boolean" />
    </declare-styleable>
    <attr name="alertFullScreenBackground" format="reference" />
    <attr name="alarmDetailsButtonStyle" format="reference" />
    <attr name="timePickerDeleteButtonStyle" format="reference" />

    <declare-styleable name="SwipeRevealLayout">
        <attr name="dragFromEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
        </attr>
    </declare-styleable>
</resources>

